{
	"name": "DF_CH_CreatEnrichedStructure_STEP4_Paruq",
	"properties": {
		"folder": {
			"name": "SLIPSCD_Enriched"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DS_SLIPLakeIngestion_CSV",
						"type": "DatasetReference"
					},
					"name": "SrcCuratedDataSets"
				}
			],
			"sinks": [
				{
					"linkedService": {
						"referenceName": "ls_sling_lake",
						"type": "LinkedServiceReference"
					},
					"name": "SnkEnrichedDataSets"
				}
			],
			"transformations": [
				{
					"name": "AddSCDFields"
				},
				{
					"name": "AlterRow1"
				}
			],
			"scriptLines": [
				"parameters{",
				"     vSinkFile as string,",
				"     PrimaryKey as string,",
				"     vContainerName as string,",
				"     vPath as string",
				"}",
				"source(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> SrcCuratedDataSets",
				"SrcCuratedDataSets derive(SURKEY = \"\",",
				"          SCDACTIVE = 1,",
				"          SCDACTIVESTARTTIME = toString(currentTimestamp(), 'dd-MM-yyyy HH:mm:ss'),",
				"          SCDACTIVEENDTIME = \"\") ~> AddSCDFields",
				"AddSCDFields alterRow(insertIf(true()),",
				"     updateIf(true())) ~> AlterRow1",
				"AlterRow1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     format: 'delta',",
				"     fileSystem: ($vContainerName),",
				"     folderPath: (concat($vPath,$vSinkFile)),",
				"     mergeSchema: false,",
				"     autoCompact: false,",
				"     optimizedWrite: false,",
				"     vacuum: 0,",
				"     deletable: false,",
				"     insertable: true,",
				"     updateable: true,",
				"     upsertable: false,",
				"     keys:[($PrimaryKey)],",
				"     partitionFileNames:[($vSinkFile)],",
				"     umask: 0777,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> SnkEnrichedDataSets"
			]
		}
	}
}