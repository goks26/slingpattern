								},
								"waitOnCompletion": true,
								"parameters": {
									"Para_PipelineRunId": {
										"value": "@pipeline().RunId",
										"type": "Expression"
									},
									"Para_JobStage": {
										"value": "Gold Stage Creation - SCD",
										"type": "Expression"
									},
									"Para_ErrorSeverity": {
										"value": "@concat(' Critical - Dataset - ',item().DataSetName)",
										"type": "Expression"
