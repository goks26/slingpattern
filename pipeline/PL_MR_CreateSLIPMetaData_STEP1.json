{
	"name": "PL_MR_CreateSLIPMetaData_STEP1",
	"properties": {
		"activities": [
			{
				"name": "AC_CPY_BLOB2AzureSQLDB",
				"description": "Move the SLIP Meta data from excel to AzureSQL Meta Data Engine",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "ExcelSource",
						"storeSettings": {
							"type": "AzureBlobStorageReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						}
					},
					"sink": {
						"type": "AzureSqlSink",
						"preCopyScript": "TRUNCATE TABLE MetaDataEngine.SLIPMetaData",
						"writeBehavior": "insert",
						"sqlWriterUseTableLock": false,
						"tableOption": "autoCreate",
						"disableMetricsCollection": false
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "DSPQ_SLIPMetaDataConfigExcel",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "DS_SLIPAzureSQLMetaDataDB_PQ",
						"type": "DatasetReference",
						"parameters": {
							"RawSLDS_Schema": "MetaDataEngine",
							"RawSLDS_Table": "SLIPMetaData"
						}
					}
				]
			},
			{
				"name": "AC_ExP_AuditTrace",
				"description": "Call  SLIPAudit Pipeline to record copy audit.",
				"type": "ExecutePipeline",
				"dependsOn": [
					{
						"activity": "AC_CPY_BLOB2AzureSQLDB",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"waitOnCompletion": true
				}
			}
		],
		"annotations": []
	}
}