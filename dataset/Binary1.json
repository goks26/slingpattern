{
	"name": "Binary1",
	"properties": {
		"linkedServiceName": {
			"referenceName": "ls_sling_blob",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"vLake": {
				"type": "string"
			},
			"vEnv": {
				"type": "string"
			},
			"vPath": {
				"type": "string"
			},
			"vDataSet": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "Binary",
		"typeProperties": {
			"location": {
				"type": "AzureBlobStorageLocation",
				"fileName": {
					"value": "@dataset().vDataSet",
					"type": "Expression"
				},
				"folderPath": {
					"value": "@dataset().vPath",
					"type": "Expression"
				},
				"container": {
					"value": "@concat(dataset().vEnv,'_',dataset().vLake)",
					"type": "Expression"
				}
			}
		}
	}
}